{
   "name": "@toba/html-webpack-inline-svg",
   "version": "1.0.10",
   "description": "Toba svg-loader module",
   "private": false,
   "author": {
      "name": "Jason Abbott",
      "url": "http://github.com/toba"
   },
   "license": "MIT",
   "publishConfig": {
      "access": "public"
   },
   "main": "cjs/index.js",
   "types:": "cjs/index.d.ts",
   "files": [
      "cjs/**/*"
   ],
   "workspaces": {
      "examples": [
         "examples/*"
      ],
      "nohoist": [
         "**/react-native",
         "**/react-native/**",
         "**/*mobile/**/react",
         "**/*mobile/**/react/**"
      ]
   },
   "scripts": {
      "prepublishOnly": "./node_modules/.bin/tsc --project tsconfig.build.json",
      "link": "ln -s ~/dev/src/toba/svg-loader/node_modules/react-native/ ~/dev/src/toba/svg-loader/node_modules/@toba/example/node_modules/",
      "test": "jest --coverage"
   },
   "devDependencies": {
      "@toba/develop": "^6.0.0",
      "@toba/example": "^3.0.0",
      "@toba/test": "^3.0.0",
      "@types/babel__core": "^7.0.5",
      "@types/babel__template": "^7.0.2",
      "@types/html-minifier": "^3.5.3",
      "@types/memory-fs": "^0.3.2",
      "@types/webpack": "^4.4.24",
      "babel-loader": "^8.0.5",
      "webpack": "^4.29.0",
      "webpack-cli": "^3.2.3"
   },
   "repository": {
      "type": "git",
      "url": "https://github.com/toba/svg-loader"
   },
   "dependencies": {
      "@svgr/core": "^4.1.0",
      "@svgr/plugin-jsx": "^4.1.0",
      "@svgr/plugin-svgo": "^4.0.3",
      "@toba/node-tools": "^1.1.0",
      "html-webpack-plugin": "^4.0.0-beta",
      "metro-react-native-babel-transformer": "^0.52.0",
      "svgo": "^1.1.1"
   }
}
